{
  "name": "n8n-nodes-keephub",
  "version": "0.1.0",
  "description": "Keephub custom nodes for n8n",
  "license": "MIT",
  "keywords": [
    "n8n-community-node-package"
  ],
  "author": {
    "name": "Niksa Perovic",
    "email": "niksa.perovic.90@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<...>/n8n-nodes-keephub.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "mkdir -p dist/nodes/Keephub && mkdir -p dist/nodes/KeephubTrigger && cp icons/Keephub.svg dist/nodes/Keephub/ && cp icons/Keephub.svg dist/nodes/KeephubTrigger/",
    "dev": "tsc -w"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/KeephubApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Keephub/Keephub.node.js"
    ]
  },
  "devDependencies": {
    "typescript": "5.9.2",
    "prettier": "3.6.2",
    "@types/node": "^20.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
